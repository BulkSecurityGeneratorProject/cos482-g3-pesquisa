<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Professor.
    -->
    <changeSet id="20171015234857-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="usuario_id"
                                 baseTableName="professor"
                                 constraintName="fk_professor_usuario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

        <addForeignKeyConstraint baseColumnNames="professors_id"
                                 baseTableName="professor_coorientador"
                                 constraintName="fk_professor_coorientador_professors_id"
                                 referencedColumnNames="id"
                                 referencedTableName="professor"/>
        <addForeignKeyConstraint baseColumnNames="coorientadors_id"
                                 baseTableName="professor_coorientador"
                                 constraintName="fk_professor_coorientador_coorientadors_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>
        
    </changeSet>
</databaseChangeLog>
