<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Aluno.
    -->
    <changeSet id="20171015234856-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="alunos_id"
                                 baseTableName="aluno_copublicacao"
                                 constraintName="fk_aluno_copublicacao_alunos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>
        <addForeignKeyConstraint baseColumnNames="copublicacaos_id"
                                 baseTableName="aluno_copublicacao"
                                 constraintName="fk_aluno_copublicacao_copublicacaos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="publicacao"/>
        
        <addForeignKeyConstraint baseColumnNames="orientador_id"
                                 baseTableName="aluno"
                                 constraintName="fk_aluno_orientador_id"
                                 referencedColumnNames="id"
                                 referencedTableName="professor"/>

    </changeSet>
</databaseChangeLog>
